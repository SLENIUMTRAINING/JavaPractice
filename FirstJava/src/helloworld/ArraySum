//Array 1 ={1, 2, 3, 4, 5} - index 0 to index 4. 
// for (int i=0; i< 4; i++)   sum = sum + array1[i];

public class ArraySum {
  public static void main (String[] args){
    int array[]={1,2,3,4,5};
    int sum=0;
    for (int i=0; i<=array.length;i++){
      sum = sum+array[i];
      
      System.out.println("The value of Sum"+sum);
      }
      
    }
}
